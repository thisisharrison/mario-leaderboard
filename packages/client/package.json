        "react-dom": "18.0.0",
        "react-redux": "8.0.2",
        "socket.io-client": "4.5.1"
    },
    "devDependencies": {
        "@types/react": "18.0.0",
        "@types/react-dom": "18.0.0",
        "@types/stylelint": "14.0.0",
        "@vitejs/plugin-react": "1.3.0",
        "sass": "1.52.3",
        "stylelint": "14.9.1",
        "stylelint-config-prettier": "9.0.3",
        "stylelint-config-standard": "26.0.0",
        "stylelint-config-standard-scss": "4.0.0",
